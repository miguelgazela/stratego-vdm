class Coord

types

values

instance variables
    public x : nat;
    inv x >= 0 and x <= 9;
    public y : nat;
    inv y >= 0 and y <= 9;

operations

-- constructor for Coord
public Coord: nat * nat ==> Coord
Coord(x_, y_) == (
  x := x_;
  y := y_;
  return self
)

functions

end Coord
